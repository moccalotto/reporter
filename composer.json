{
    "name": "moccalotto/reporter",
    "type": "cli program",
    "description": "CLI program for reporting on system stats",
    "keywords": ["monitoring", "server", "status", "reporting"],
    "require": {
        "php": ">=5.6.0",
        "pimple/pimple": "~3.0",
        "nategood/commando": "^0.2.7"
    },
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Kim Ravn Hansen",
            "email": "moccalotto@gmail.com"
        }
    ],
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Moccalotto\\Reporter\\": "src"
        }
    },
    "archive": {
        "exclude": ["/spec", "/demo.php"]
    }
}
