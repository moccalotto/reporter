{
    "report": {
        "uri": "https:\/\/httpbin.org\/post"
    },
    "logging": {
        "minLevel": "warning"
    },
    "daemon": {
        "enabled": false,
        "interval": 300
    },
    "signing": {
        "key": "CHANGEME",
        "algorithm": "sha256"
    },
    "http": {
        "follow_location": true,
        "max_redirects": 20,
        "user_agent": "Reporter",
        "timeout": 10
    },
    "https": {
        "verify_peer": true,
        "verify_peer_name": true,
        "allow_self_signed": false
    }
}
